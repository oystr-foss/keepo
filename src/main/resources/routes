# Route to query/delete credentials and certificates.
GET    /secrets/:bot/:username/:target  oystr.controllers.SecretsController.dataFor(bot: String, username: String, target: String)
DELETE /secrets/:bot/:username/:target  oystr.controllers.SecretsController.deleteDataFor(bot: String, username: String, target: String)

# Request to handle policies, tokens and secrets.
POST   /policy                                              oystr.controllers.SecretsController.createPolicyFor()
POST   /token                                               oystr.controllers.SecretsController.createTokenFor()
POST   /secrets                                             oystr.controllers.SecretsController.storeCredentialsFor()
